{
  "presets": [["es2015", { "modules": false }], "stage-0", "react"],
  "plugins": [
    "syntax-dynamic-import",
    "babel-plugin-styled-components",
    ["module-resolver", { "root": ["./src"], "alias": { "data": "./src/data" } }]
  ],
  "env": {
    "development": {
      "presets": [["es2015", { "modules": false }], "stage-0", "react"],
      "plugins": [
        "syntax-dynamic-import",
        "babel-plugin-styled-components",
        ["module-resolver", { "root": ["./src"], "alias": { "data": "./src/data" } }],
        "react-hot-loader/babel"
      ]
    }
  }
}
